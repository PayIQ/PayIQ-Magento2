<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <payiq>
                <active>1</active>
                <model>PayIQ\Magento2\Model\Method\PayIQ</model>
                <title>PayIQ</title>
                <debug>1</debug>
                <accountnumber>payiq account number</accountnumber>
                <encryptionkey>payiq encryption key</encryptionkey>
                <transactiontype>SALE</transactiontype>
                <payment_type>PX</payment_type>
                <order_status_authorize>processing</order_status_authorize>
                <order_status_capture>processing</order_status_capture>
                <responsive>1</responsive>
                <checkoutinfo>1</checkoutinfo>
                <language>en-US</language>
                <allowspecific>0</allowspecific>
                <group>payiq</group>
            </payiq>
        </payment>
    </default>
</config>
